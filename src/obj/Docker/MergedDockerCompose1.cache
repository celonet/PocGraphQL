services:
  app:
    build:
      context: C:\Users\celo-\Documents\Projetos\PocGraphicQL
      dockerfile: PocGraphicQL/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: PocGraphicQL
      target: base
    container_name: PocGraphicQL
    depends_on:
    - database
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages;/root/.nuget/fallbackpackages2
    image: pocgraphicql:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages --additionalProbingPath
        /root/.nuget/fallbackpackages2  "bin/Debug/netcoreapp3.1/PocGraphicQL.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    tty: true
    volumes:
    - C:\Users\celo-\Documents\Projetos\PocGraphicQL\PocGraphicQL:/app:rw
    - C:\Users\celo-\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\celo-\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\celo-\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Microsoft\Xamarin\NuGet:/root/.nuget/fallbackpackages:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages2:ro
    - C:\Users\celo-\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\celo-\Documents\Projetos\PocGraphicQL:/src:rw
  database:
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: <YourStrong@Passw0rd>
    image: mcr.microsoft.com/mssql/server:2019-GA-ubuntu-16.04
    ports:
    - published: 1433
      target: 1433
version: '3.4'